al":{"line":88,"column":7}},{"generated":{"line":117,"column":43},"source":"SearchParams.js","original":{"line":88,"column":6}},{"generated":{"line":118,"column":0},"source":"SearchParams.js","original":{"line":88,"column":15}},{"generated":{"line":118,"column":8},"source":"SearchParams.js","original":{"line":88,"column":15}},{"generated":{"line":118,"column":12},"source":"SearchParams.js","original":{"line":88,"column":19}},{"name":"pets","generated":{"line":118,"column":14},"source":"SearchParams.js","original":{"line":88,"column":21}},{"generated":{"line":119,"column":0},"source":"SearchParams.js","original":{"line":88,"column":6}},{"generated":{"line":119,"column":8},"source":"SearchParams.js","original":{"line":33,"column":4}},{"generated":{"line":120,"column":0},"source":"SearchParams.js","original":{"line":33,"column":4}},{"generated":{"line":121,"column":0},"source":"SearchParams.js","original":{"line":32,"column":2}},{"generated":{"line":122,"column":0},"source":"SearchParams.js","original":{"line":91,"column":1}},{"generated":{"line":122,"column":1},"source":"SearchParams.js","original":{"line":9,"column":0}},{"name":"SearchParams","generated":{"line":124,"column":15},"source":"SearchParams.js","original":{"line":93,"column":15}},{"generated":{"line":124,"column":29},"source":"SearchParams.js","original":{"line":95,"column":0}},{"generated":{"line":125,"column":0},"source":"SearchParams.js","original":{"line":96,"column":0}}],"sources":{"SearchParams.js":"import { useState, useEffect } from 'react';\nimport useBreedList from './useBreedList';\nimport Results from './Results';\n// useState is a hook that allows us to keep track of state as indicated by the state\n// hooks always begins with 'use'\n\nconst ANIMALS = [\"bird\", \"cat\", \"dog\", \"rabbit\", \"reptile\"];\n\nconst SearchParams = () => {\n  const [location, setLocation] = useState(\"\"); // this is a hook that allows us to have the location and setLocation will track it over time\n  const [animal, setAnimal] = useState(\"\");\n  const [breed, setBreed] = useState(\"\");\n  const [pets, setPets] = useState([]);\n  const [breedList] = useBreedList(animal);\n\n  useEffect(() => {\n    requestPets();\n  }, []); // eslint-disable-line react-hooks/exhaustive-deps\n  // ðŸ‘† this comment tells eslint to ignore the rule for this particular line only\n  // this [] is telling the effect to always run once at the beginning and then when to rerun. Without it, it creates an infinite loop whenever we call setPets\n\n\n  async function requestPets() {\n    const res = await fetch(\n      `http://pets-v2.dev-apis.com/pets?animal=${animal}&location=${location}&breed=${breed}` // this is the API\n      );\n      const json = await res.json(); // this json is whatever we get back from the API\n      setPets(json.pets);\n  // so everytime we call requestPets(), it's gonna take all the data that I have, and also grab whatever we were searching for from the API\n  }\n\n  return (\n    <div className=\"search-params\">\n      <form\n        onSubmit={e => {\n          e.preventDefault();\n          requestPets();\n        }}\n      >\n        <label htmlFor=\"location\">\n          Location\n          <input id=\"location\"\n          onChange={(e) => setLocation(e.target.value)} // 1 line arrow function\n          // whenever user does something, it will give us back a function that gives us back an event\n          value={location}\n          placeholder=\"Location\" />\n        </label>\n\n        <label htmlFor=\"animal\">\n          Animal\n          <select id=\"animal\"\n          onChange={(e) => setAnimal(e.target.value)} // with only onChange here, it gives an error to use onBlur instead as it worries onChange might not be enough to capture keyboard or screen reader users' response\n          onBlur={(e) => setAnimal(e.target.value)}\n          value={animal} >\n\n          <option />\n          {/* // same as <option value=\"\"></option> */}\n          {\n            ANIMALS.map(animal => (\n              <option value={animal} key={animal}>\n                {animal}\n              </option>\n            ))\n          }\n          </select>\n        </label>\n\n        <label htmlFor=\"breed\">\n          Breed\n          <select id=\"breed\"\n          onChange={(e) => setBreed(e.target.value)} // with only onChange here, it gives an error to use onBlur instead as it worries onChange might not be enough to capture keyboard or screen reader users' response\n          onBlur={(e) => setBreed(e.target.value)}\n          value={breed} >\n\n          <option />\n          {\n            breedList.map(breed => (\n              <option value={breed} key={breed}>\n                {breed}\n              </option>\n            ))\n          }\n          </select>\n        </label>\n\n        <button>Submit</button>\n      </form>\n      <Results pets={pets} />\n    </div>\n  )\n}\n\nexport default SearchParams;\n\n// the word class is a reserved word hence we use className\n// For is reserved for for loops in JS hence we use htmlFor\n"},"lineCount":null}},"error":null,"hash":"37150812beb68db5f38598b64eda2b4f","cacheData":{"env":{}}}