:44,"column":8}},{"generated":{"line":85,"column":10},"source":"SearchParams.js","original":{"line":15,"column":6}},{"generated":{"line":86,"column":0},"source":"SearchParams.js","original":{"line":15,"column":6}},{"generated":{"line":87,"column":0},"source":"SearchParams.js","original":{"line":62,"column":8}},{"generated":{"line":88,"column":0},"source":"SearchParams.js","original":{"line":62,"column":8}},{"generated":{"line":89,"column":0},"source":"SearchParams.js","original":{"line":62,"column":8}},{"generated":{"line":89,"column":10},"source":"SearchParams.js","original":{"line":15,"column":6}},{"generated":{"line":90,"column":0},"source":"SearchParams.js","original":{"line":15,"column":6}},{"generated":{"line":91,"column":0},"source":"SearchParams.js","original":{"line":14,"column":4}},{"generated":{"line":92,"column":0},"source":"SearchParams.js","original":{"line":13,"column":2}},{"generated":{"line":93,"column":0},"source":"SearchParams.js","original":{"line":66,"column":1}},{"generated":{"line":93,"column":1},"source":"SearchParams.js","original":{"line":7,"column":0}},{"name":"SearchParams","generated":{"line":95,"column":15},"source":"SearchParams.js","original":{"line":68,"column":15}},{"generated":{"line":95,"column":29},"source":"SearchParams.js","original":{"line":70,"column":0}},{"generated":{"line":96,"column":0},"source":"SearchParams.js","original":{"line":71,"column":0}}],"sources":{"SearchParams.js":"import { useState } from 'react';\n// useState is a hook that allows us to keep track of state as indicated by the state\n// hooks always begins with 'use'\n\nconst ANIMALS = [\"bird\", \"cat\", \"dog\", \"rabbit\", \"reptile\"];\n\nconst SearchParams = () => {\n  const [location, setLocation] = useState(\"Seattle, WA\"); // this is a hook that allows us to have the location and setLocation will track it over time\n  const [animal, setAnimal] = useState(\"\");\n  const [breed, setBreed] = useState(\"\");\n  const breeds = [];\n\n  return (\n    <div className=\"search-params\">\n      <form>\n        <label htmlFor=\"location\">\n          Location\n          <input id=\"location\"\n          onChange={(e) => setLocation(e.target.value)} // 1 line arrow function\n          // whenever user does something, it will give us back a function that gives us back an event\n          value={location}\n          placeholder=\"Location\" />\n        </label>\n\n        <label htmlFor=\"animal\">\n          Animal\n          <select id=\"animal\"\n          onChange={(e) => setAnimal(e.target.value)} // with only onChange here, it gives an error to use onBlur instead as it worries onChange might not be enough to capture keyboard or screen reader users' response\n          onBlur={(e) => setAnimal(e.target.value)}\n          value={animal} >\n\n          <option />\n          {/* // same as <option value=\"\"></option> */}\n          {\n            ANIMALS.map(animal => (\n              <option value={animal} key={animal}>\n                {animal}\n              </option>\n            ))\n          }\n          </select>\n        </label>\n\n        <label htmlFor=\"breed\">\n          Breed\n          <select id=\"breed\"\n          onChange={(e) => setBreed(e.target.value)} // with only onChange here, it gives an error to use onBlur instead as it worries onChange might not be enough to capture keyboard or screen reader users' response\n          onBlur={(e) => setBreed(e.target.value)}\n          value={breed} >\n\n          <option />\n          {\n            breeds.map(breed => (\n              <option value={breed} key={breed}>\n                {breed}\n              </option>\n            ))\n          }\n          </select>\n        </label>\n\n        <button>Submit</button>\n      </form>\n    </div>\n  )\n}\n\nexport default SearchParams;\n\n// the word class is a reserved word hence we use className\n// For is reserved for for loops in JS hence we use htmlFor\n"},"lineCount":null}},"error":null,"hash":"0dcca5